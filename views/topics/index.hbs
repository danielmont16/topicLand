<h1>{{title}}</h1>

{{!-- Add links to CRUD operations --}}
<a href="/topics/add">Add a new Topic</a>

{{!-- To Show data --}}
<table class="table table-striped table-hover">
  <thead>
    <th>Name</th>
    <th>Description</th>
    <th>Creation Date</th>
  </thead>
  <tbody>
  {{#each dataset}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.description}}</td>
      <td>{{this.creationDate}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>